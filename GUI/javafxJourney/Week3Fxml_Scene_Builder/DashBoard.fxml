<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="552.0" prefWidth="800.0" spacing="10" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.javafxJourney.Week3Fxml_Scene_Builder.DashBoard">
  <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
 <children>
      <VBox alignment="TOP_CENTER" focusTraversable="true" prefHeight="531.0" prefWidth="779.0">
         <children>
            <Label fx:id="welcomeLabel" contentDisplay="TOP" text="Welcome to App" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="34.0" />
               </font>
            </Label>
            <Label fx:id="userName" text="hello">
               <font>
                  <Font name="Candara Light" size="26.0" />
               </font>
               <VBox.margin>
                  <Insets top="100.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" opacity="0.5" text="logout" fx:id="logoutBtn" onAction="#handleLogout">
               <VBox.margin>
                  <Insets top="100.0" />
               </VBox.margin>
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <opaqueInsets>
            <Insets top="10.0" />
         </opaqueInsets>
      </VBox>
   </children>
</VBox>
